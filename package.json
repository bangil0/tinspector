{
  "name": "tinspector",
  "version": "2.1.2",
  "description": "TypeScript type inspector",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "prepublish": "tsc -p tsconfig.build.json"
  },
  "keywords": [
    "Reflection",
    "Reflect",
    "Metadata"
  ],
  "author": "Ketut Sandiarsa",
  "license": "MIT",
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "tslib": "^1.9.3",
    "typescript": "^3.2.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "snapshotSerializers": [
      "./scripts/function-snapshot-serializer.js"
    ]
  }
}
